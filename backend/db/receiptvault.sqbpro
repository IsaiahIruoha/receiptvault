    budget_id INTEGER PRIMARY KEY AUTOINCREMENT,
    user_id INTEGER NOT NULL,           -- User associated with the budget
    category_id INTEGER NOT NULL,       -- Category this budget applies to
    budget_amount REAL NOT NULL,        -- Budget amount set for the category
